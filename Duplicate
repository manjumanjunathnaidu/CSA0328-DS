#include<stdio.h>
int main()
{
int n,i,j,position,a[10],key;
printf("Enter the size of array:");
scanf("%d",&n);
printf("Enter the elements of array\n");
for(i=0;i<n;i++)
scanf("%d",&a[i]);
printf("Duplicate elements are:");

for(i=0;i<n-1;i++)
for (j=i+1;j<n;j++)
{
if (a[i]==a[j])
{
printf("Duplicate value present at the position %d%d",i+1,j+1);
}
}

return 0;
}
